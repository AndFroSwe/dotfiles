{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "lavender": "#B7BDF8",
    "blue": "#8AADF4",
    "white": "#FFFFFF",
    "text": "#494D64",
    "battery_charge": "#FFDFD3",
    "battery_low": "#BB5500",
    "battery_full": "#000000"
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:white",
    "template": " "
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "background": "p:blue",
          "foreground": "p:white",
          "leading_diamond": "\u256d\u2500",
          "trailing_diamond": "",
          "properties": {
            "style": "agnoster",
            "home_icon": " ",
            "folder_icon": "",
            "folder_separator_icon": " / ",
            "mapped_locations": {
              "~/.config": "",
              "~/Gitrepos": "󰊢"
            }
          },
          "style": "diamond",
          "template": " {{ .Path }} "
        },
        {
          "type": "cmake",
          "style": "diamond",
          "trailing_diamond": "",
          "foreground": "p:blue",
          "background": "p:white",
          "template": "   cmake "
        },
        {
          "type": "git",
          "background": "p:lavender",
          "foreground": "p:white",
          "style": "diamond",
          "trailing_diamond": "",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:text{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:text{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}",
            "{{ if gt .Behind 0 }}p:white{{ end }}"
          ],
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{if .BranchStatus }}{{ .BranchStatus }}  {{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "",
          "foreground": "p:text",
          "background": "p:pink",
          "background_templates": [
            "{{if lt .Percentage 25}}p:battery_low{{end}}",
            "{{if eq \"Charging\" .State.String}}p:battery_charge{{end}}",
            "{{if eq \"Full\" .State.String}}p:battery_full{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }} ",
          "properties": {
            "discharging_icon": "󱟤 ",
            "charging_icon": "󰂅 ",
            "charged_icon": "󱊣 "
          }
        },
        {
          "type": "sysinfo",
          "foreground": "p:white",
          "background": "p:blue",
          "leading_diamond": "",
          "style": "diamond",
          "template": "   {{ round .PhysicalPercentUsed .Precision }}% ",
          "properties": {
            "precision": 2
          }
        },
        {
          "type": "time",
          "foreground": "p:white",
          "background": "p:lavender",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": "  {{ .CurrentDate | date .Format }} "
        },
        {
          "type": "executiontime",
          "background": "p:os",
          "foreground": "p:text",
          "leading_diamond": "",
          "trailing_diamond": "",
          "properties": {
            "style": "austin",
            "threshold": 300
          },
          "style": "diamond",
          "template": "  {{ .FormattedMs }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#0c7bbb",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
